{
  "name": "@quickts/hrvp",
  "version": "1.0.0",
  "description": "A quickts project",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:prod": "node dist/index.js",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register --recursive -R spec test/*.ts"
  },
  "keywords": [
    "quickts",
    "hrvp"
  ],
  "author": "quickts",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "tsconfig-paths": "^3.8.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quickts/hrvp.git"
  },
  "bugs": "https://github.com/quickts/hrvp/issues",
  "homepage": "https://github.com/quickts/hrvp#readme"
}
